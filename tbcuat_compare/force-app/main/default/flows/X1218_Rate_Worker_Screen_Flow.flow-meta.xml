<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <apiVersion>64.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <description>Sets the RatedByContactId to the User.ContactId field if the user has a contact id</description>
        <name>Set_community_user_contact_in_variable</name>
        <label>Set community user contact in variable</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>ratedByContactId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$User.ContactId</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>CreateRating</targetReference>
        </connector>
    </assignments>
    <customProperties>
        <name>ScreenProgressIndicator</name>
        <value>
            <stringValue>{&quot;location&quot;:&quot;top&quot;,&quot;type&quot;:&quot;simple&quot;}</stringValue>
        </value>
    </customProperties>
    <decisions>
        <name>foundShift</name>
        <label>foundShift?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>No_Shift_found</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes_Shift_found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>getShift</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Contact_for_shift</targetReference>
            </connector>
            <label>Yes - Shift found</label>
        </rules>
    </decisions>
    <decisions>
        <name>IsCommunityUser</name>
        <label>IsCommunityUser?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>CreateRating_Non_community_user</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes_Community_User</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$User.ContactId</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_community_user_contact_in_variable</targetReference>
            </connector>
            <label>Yes Community User</label>
        </rules>
    </decisions>
    <decisions>
        <name>isRecordIdProvided</name>
        <label>isRecordIdProvided?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>recordId_provided</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recordIds</leftValueReference>
                <operator>IsEmpty</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>getShift</targetReference>
            </connector>
            <label>recordId provided</label>
        </rules>
    </decisions>
    <dynamicChoiceSets>
        <name>RatingOptions</name>
        <dataType>Picklist</dataType>
        <displayField xsi:nil="true"/>
        <object xsi:nil="true"/>
        <picklistField>sirenum__Rank__c</picklistField>
        <picklistObject>sirenum__Ranking__c</picklistObject>
    </dynamicChoiceSets>
    <environments>Default</environments>
    <interviewLabel>1218 - Rate Worker Screen Flow {!$Flow.CurrentDateTime}</interviewLabel>
    <label>1218 - Rate Worker Screen Flow</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>Flow</processType>
    <recordCreates>
        <name>CreateRating</name>
        <label>CreateRating</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>set_has_client_rating_on_shift</targetReference>
        </connector>
        <inputAssignments>
            <field>Shift__c</field>
            <value>
                <elementReference>getShift.Id</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>sirenum__Account__c</field>
            <value>
                <elementReference>getShift.sirenum__Client_ID__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>sirenum__Contact__c</field>
            <value>
                <elementReference>getShift.sirenum__Contact__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>sirenum__Job_Type__c</field>
            <value>
                <elementReference>getShift.sirenum__Job_Type__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>sirenum__Rank__c</field>
            <value>
                <elementReference>Rating</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>sirenum__Ranked_by__c</field>
            <value>
                <elementReference>ratedByContactId</elementReference>
            </value>
        </inputAssignments>
        <object>sirenum__Ranking__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordCreates>
    <recordCreates>
        <name>CreateRating_Non_community_user</name>
        <label>CreateRating - Non community user</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>RatingSubmitted</targetReference>
        </connector>
        <inputAssignments>
            <field>Shift__c</field>
            <value>
                <elementReference>getShift.Id</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>sirenum__Account__c</field>
            <value>
                <elementReference>getShift.sirenum__Client_ID__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>sirenum__Contact__c</field>
            <value>
                <elementReference>getShift.sirenum__Contact__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>sirenum__Job_Type__c</field>
            <value>
                <elementReference>getShift.sirenum__Job_Type__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>sirenum__Rank__c</field>
            <value>
                <elementReference>Rating</elementReference>
            </value>
        </inputAssignments>
        <object>sirenum__Ranking__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordCreates>
    <recordLookups>
        <name>Get_Contact_for_shift</name>
        <label>Get Contact for shift</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Rate_worker</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>getShift.sirenum__Contact__c</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Contact</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordLookups>
        <description>Gets only the first record from the set as the schedule definition restricts this to one value.</description>
        <name>getShift</name>
        <label>getShift</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>foundShift</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>In</operator>
            <value>
                <elementReference>recordIds</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>sirenum__Shift__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordUpdates>
        <name>set_has_client_rating_on_shift</name>
        <label>set has client rating on shift</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>RatingSubmitted</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>getShift.Id</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>HasClientRating__c</field>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </inputAssignments>
        <object>sirenum__Shift__c</object>
    </recordUpdates>
    <runInMode>SystemModeWithoutSharing</runInMode>
